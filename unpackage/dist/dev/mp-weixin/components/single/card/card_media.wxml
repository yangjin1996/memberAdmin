<view class="cu-card bg-white margin-line data-v-198e0b08"><block wx:for="{{datas}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{item.is_geted!=1}}"><view class="cu-item bg-img shadow-blur data-v-198e0b08" style="{{'background-image:'+('url(https://image.weilanwl.com/color2.0/plugin/qpct2148.jpg)')+';'}}" data-id="{{index}}" data-event-opts="{{[['tap',[['onClickItem',['$event']]]]]}}" bindtap="__e"><view class="cardTitle data-v-198e0b08"><text class="padding-left text-size-28  data-v-198e0b08">{{item.name+"抵用劵"}}</text><block wx:if="{{item.emoney>0}}"><text class="padding-left text-size-30  data-v-198e0b08">{{item.emoney+"—到店优惠-"}}</text></block><text class="padding-left text-size-50  data-v-198e0b08">{{item.price}}</text></view><block wx:if="{{selectId==index}}"><text class="cuIcon-roundcheckfill text-red data-v-198e0b08"></text></block></view></block></block></view>