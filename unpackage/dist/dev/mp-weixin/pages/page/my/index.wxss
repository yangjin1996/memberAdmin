
.cuIcon {
	font-size: 50rpx;
}
.center {
	margin: auto auto;
}
.center-list {
	background-color: #ffffff;
	margin-top: 15rpx;
	margin-bottom: 15rpx;
	width: 100%;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
}
.center-list-item {
	height: 90rpx;
	width: 100%;
	box-sizing: border-box;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	        flex-direction: row;
	padding: 0rpx 20rpx;
	position: relative;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.border-bottom {
	border-bottom-width: 1rpx;
	border-color: #c8c7cc;
	border-bottom-style: solid;
}
.list-icon {
	width: 40rpx;
	height: 40rpx;
	line-height: 90rpx;
	font-size: 34rpx;
	color: #2f85fc;
	text-align: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	font-family: texticons;
	margin-right: 20rpx;
	vertical-align: text-bottom;
}
.list-text {
	height: 90rpx;
	line-height: 90rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	text-align: left;
	font-size: 30rpx;
	font-family: FZLTKHK--GBK1-0;
	font-weight: 400;
	color: rgba(131, 131, 131, 1);
}
.navigat-arrow {
	height: 90rpx;
	width: 40rpx;
	line-height: 90rpx;
	font-size: 34rpx;
	color: #555;
	text-align: right;
	font-family: texticons;
	position: absolute;
	right: 40rpx;
}

