.title-cell {
		width: 100%;
		padding: 20rpx;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		margin-top: 2rpx;
		margin-bottom: 2rpx;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		background-color: white;
		padding-left: 30rpx;
		padding-right: 30rpx;
		
		border-top: solid #DFDFDF 1rpx;
}
.title-cell-title {
		width: auto;
		font-size: 30rpx;
}
.title-cell-tag {
		background-color: #F4C249;
		border: 1px solid white;
		border-radius: 16rpx;
		padding-top: 4rpx;
		padding-bottom: 4rpx;
		padding-left: 12rpx;
		padding-right: 12rpx;
		margin-left: auto;
		margin-right: 0;
		font-family: FZLTKHK--GBK1-0;
		font-weight: 400;
		color: rgba(160, 91, 18, 1);
}
.steps-desc-text {
		text-align: left;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
		width: 100%;
		padding: 10rpx;
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
		border-bottom: solid 1rpx #DCDCDC;
}
.steps_right {
		width: 100%;
		text-align: right;
		padding-right: 20rpx;
		font-size: 30rpx;
		padding-top: 8rpx;
		padding-bottom: 8rpx;
		line-height: 30rpx;
		font-family: FZLTKHK--GBK1-0;
		font-weight: 400;
		color: rgba(202, 200, 200, 1);
		-webkit-box-align: center;
		-webkit-align-items: center;
		        align-items: center;
}
.steps_left {
		width: 300rpx;
		font-size: 30rpx;
		font-family: FZLTKHK--GBK1-0;
		font-weight: 400;
		padding: 8rpx;
		text-align: center;
		color: rgba(154, 152, 152, 1);
}
.page-images{
	padding: 3rem;
	background-color: #FFFFFF;
	text-align: center;
}
.page-image {
	width: 200rpx;
	height: 200rpx;
}
.list-title-cell {
	position: relative;
	background: #FFFFFF;
	display: block;
	width: 100%;
}
.title-cell:after {
	content: '';
	width: 100%;
	height: 1px;
	background-color: #dcdcdc;
	display: block;
	left: 100rpx;
	position: absolute;
	bottom: 0;
}
.page-text{
	text-align: center;
	font-size: 35rpx;
}
.title-cell-tag{
	width: 350rpx;
}
.title-cell-desc{
	width: 80%;
	font-size: 26rpx;
	text-align: right;
}
.desc-address{
	width: 300rpx;
	right: 1rem;
	position: absolute;
}
.container{
	background-color: #FFFFFF;
}

